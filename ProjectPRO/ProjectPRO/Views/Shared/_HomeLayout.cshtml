
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link href="/Content/materialize.min.css" rel="stylesheet" type="text/css" />
    <script src="/Scripts/modernizr-2.8.3.js"></script>
</head>
<body>
 <nav>
  <ul class="left hide-on-med-and-down">
      <li>@Html.ActionLink("Add Groups", "AddGroups", "Home")</li>
      <li>@Html.ActionLink("Create new groups", "CreateG", "Home")</li>
      <li>@Html.ActionLink("Discussions", "Discussions", "Discussion")</li>
      <li>@Html.ActionLink("Files", "Files", "File")</li>
  </ul>
 </nav>


<div class="parallax-container">
    @RenderBody()
    <footer class="page-footer">
        <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
    </footer>
</div>

<script src="/Scripts/jquery-3.1.1.min.js"></script>
<script src="/Scripts/materialize.min.js"></script>
</body>
</html>